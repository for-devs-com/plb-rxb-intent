                                        <xs:element name="ProductType" type="xs:unsignedByte"/>
                                        <xs:element name="PrdHierarchyInd" type="xs:unsignedByte"/>
                                        <xs:element name="AttributeDtlsTbl">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="AttributeDtls">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="AttributeName" type="xs:string"/>
                                                                <xs:element name="AttributeValue" type="xs:string"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="EffectiveDate" type="xs:string"/>
                                        <xs:element name="EndDate" type="xs:string"/>
                                        <xs:element name="Channel" type="xs:unsignedByte"/>
                                        <xs:element name="OperId" type="xs:string"/>
                                        <xs:element name="Timestamp" type="xs:string"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>