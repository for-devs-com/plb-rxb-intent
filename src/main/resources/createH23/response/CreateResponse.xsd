                                    <xs:sequence>
                                        <xs:element name="EffDateOfchange" type="xs:string"/>
                                        <xs:element name="Operation" type="xs:string"/>
                                        <xs:element name="RuleStatusCode" type="xs:string"/>
                                        <xs:element name="RuleStatusMessage" type="xs:string"/>
                                        <xs:element name="RuleAgnID" type="xs:unsignedInt"/>
                                        <xs:element name="ParentAgn" type="xs:unsignedInt"/>
                                        <xs:element name="ClientAgn" type="xs:unsignedInt"/>
                                        <xs:element name="EntityExclusionInd" type="xs:string"/>
                                        <xs:element name="EntityExclusionTbl">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="EntityExclusion">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="ClientExclAgnID"
                                                                            type="xs:unsignedInt"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="NtwPhmName" type="xs:string"/>
