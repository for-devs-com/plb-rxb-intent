                                        <xs:element name="State" type="xs:string"/>
                                        <xs:element name="PhmChnName" type="xs:string"/>
                                        <xs:element name="PhmNetworkExclInd" type="xs:string"/>
                                        <xs:element name="PhmNetworkExclTbl" minOccurs="0" maxOccurs="unbounded"/>
